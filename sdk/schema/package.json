{
  "name": "@webnode-ecosystem/schema",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "tags": [
      "scope:webnode",
      "type:publishable"
    ],
    "sourceRoot": "sdk/schema/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "sdk/schema/dist",
          "main": "sdk/schema/src/index.ts",
          "tsConfig": "sdk/schema/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  }
}
