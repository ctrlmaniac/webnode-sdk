{
  "name": "@webnode-ecosystem/types",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "@webnode-ecosystem/schema": "workspace:^",
    "zod": "^3.25.20"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "projectType": "library",
    "tags": [
      "access:public",
      "domain:sdk",
      "type:type-definition"
    ],
    "sourceRoot": "sdk/types/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "sdk/types/dist",
          "main": "sdk/types/src/index.ts",
          "tsConfig": "sdk/types/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  }
}
